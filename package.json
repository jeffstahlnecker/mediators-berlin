{
  "name": "gatsby-stahlnecker-starter",
  "private": true,
  "description": "A simple starter including essential plugins, with eslint and jest configured",
  "version": "0.0.1",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "validate": "npm-run-all --parallel format lint build"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "lint-staged": {
    "**/*.+(js|json|css|html|md)": [
      "prettier",
      "jest --findRelatedTests"
    ]
  },
  "jest-runner-eslint": {
    "clipOptions": {
      "ignorePath": "./.eslintignore"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.4.17",
    "@directus/gatsby-source-directus": "9.0.0-rc.95",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@tailwindcss/ui": "^0.7.2",
    "@tailwindui/react": "^0.1.1",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "apollo-boost": "^0.4.9",
    "axios": "^0.24.0",
    "babel-plugin-styled-components": "^1.13.3",
    "dotenv": "^10.0.0",
    "gatsby": "^4.2.0",
    "gatsby-background-image": "^1.5.3",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-apollo": "^3.0.2",
    "gatsby-plugin-cookiehub": "^1.0.0",
    "gatsby-plugin-cookiehub-banner": "^0.1.0",
    "gatsby-plugin-emotion": "^7.2.0",
    "gatsby-plugin-image": "^2.2.0",
    "gatsby-plugin-postcss": "^5.2.0",
    "gatsby-plugin-react-helmet": "^5.2.0",
    "gatsby-plugin-react-i18next": "^1.2.2",
    "gatsby-plugin-sharp": "^4.2.0",
    "gatsby-plugin-sitemap": "^5.2.0",
    "gatsby-plugin-styled-components": "^5.2.0",
    "gatsby-plugin-svg-sprite": "^2.0.1",
    "gatsby-source-filesystem": "^4.2.0",
    "gatsby-source-graphql": "^4.2.0",
    "gatsby-transformer-remark": "^5.2.0",
    "gatsby-transformer-sharp": "^4.2.0",
    "i18next": "^21.5.2",
    "isomorphic-fetch": "^3.0.0",
    "jest-axe": "^5.0.1",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.14.2",
    "react-icons": "^4.3.1",
    "react-query": "^3.33.1",
    "styled-components": "^5.3.3",
    "stylelint": "^14.1.0",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^2.2.19",
    "twin.macro": "^2.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "babel-preset-gatsby": "^2.2.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^8.0.5",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.3.1",
    "jest-emotion": "^11.0.0",
    "jest-runner-eslint": "^1.0.0",
    "jest-styled-components": "^7.0.6",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffstahlnecker/gatsby-stahlnecker-starter.git"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
