{
  "name": "gatsby-stahlnecker-starter",
  "private": true,
  "description": "A simple starter including essential plugins, with eslint and jest configured",
  "version": "0.0.1",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "validate": "npm-run-all --parallel format lint build"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "lint-staged": {
    "**/*.+(js|json|css|html|md)": [
      "prettier",
      "jest --findRelatedTests"
    ]
  },
  "jest-runner-eslint": {
    "clipOptions": {
      "ignorePath": "./.eslintignore"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.3.3",
    "@directus/gatsby-source-directus": "^0.2.9",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@tailwindcss/ui": "^0.5.0",
    "@tailwindui/react": "^0.1.1",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "apollo-boost": "^0.4.9",
    "axios": "^0.21.0",
    "babel-plugin-styled-components": "^1.11.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.47",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-apollo": "^3.0.2",
    "gatsby-plugin-cookiehub-banner": "^0.1.0",
    "gatsby-plugin-emotion": "^4.3.10",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-svg-sprite": "^2.0.1",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-source-graphql": "^2.9.0",
    "gatsby-source-notion-contents": "^1.1.3",
    "gatsby-source-notionso": "^1.1.6",
    "gatsby-transformer-remark": "^2.8.34",
    "gatsby-transformer-sharp": "^2.5.13",
    "isomorphic-fetch": "^3.0.0",
    "jest-axe": "^3.5.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-query": "^2.26.3",
    "styled-components": "^5.2.0",
    "stylelint": "^13.7.0",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.5.1",
    "twin.macro": "^1.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-preset-gatsby": "^0.5.5",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.4.2",
    "jest-emotion": "^10.0.32",
    "jest-runner-eslint": "^0.10.0",
    "jest-styled-components": "^7.0.3",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffstahlnecker/gatsby-stahlnecker-starter.git"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
